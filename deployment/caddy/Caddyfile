psyclinicai.com {
  root * /usr/share/caddy
  encode gzip zstd

  @assets {
    path /assets/*
    path /icons/*
    path *.js
    path *.css
    path *.png
    path *.jpg
    path *.svg
    path *.woff2
  }

  handle @assets {
    file_server
  }

  handle {
    try_files {path} /index.html
    file_server
  }
}

